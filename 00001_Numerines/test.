describe "numerines" {
  const numerines = [3,5,8,22,9,6,15]

  test "filter" {
    assert.that(
      numerines.filter { n => n.between(7,17) } ==
      resultados.filter()
    )
  }
  
  test "any" {
    assert.that(
      numerines.any { n => n.between(7,17) } == 
      resultados.any()
    )
  }
  
  test "find" {
    assert.that(
      numerines.find { n => n.between(7,17) } == 
      resultados.find()
    )
  }
  
  test "all" {
    assert.that(
      numerines.all { n => n.between(7,17) } == 
      resultados.all()
    )
  }
  
  test "count" {
    assert.that(
      numerines.count { n => n.between(7,17) } == 
      resultados.count()
    )
  }
}